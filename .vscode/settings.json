{
    "python.pythonPath": "env\\Scripts\\python.exe",
    "python.jediEnabled": false,
    "autoDocstring.docstringFormat": "google",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": {
            "when": "$(basename).py"
        },
        "**/__pycache__": true
    }
}